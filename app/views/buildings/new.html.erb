<%= form_with scope: :buildings, url: buildings_path, local: true do |form| %>
    <p>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
    </p>

    <p>
      <%= form.label :code %><br>
      <%= form.text_field :code %>
    </p>


    <p>
      <%= form.submit %>
    </p>
<% end %>